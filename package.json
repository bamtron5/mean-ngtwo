{
  "name": "claimBook",
  "version": "0.0.10.pre",
  "private": true,
  "scripts": {
    "serve": "DEBUG=claimBook* && node ./bin/www",
    "prune": "rm -rf appBuilt routesBuilt testBuilt",
    "start": "npm run prune & node_modules/typescript/bin/tsc & npm run serve",
    "debug": "node-debug bin/www",
    "test": "node_modules/jasmine-node/bin/jasmine-node --verbose testBuilt/test/*.js",
    "testBuild": "rm -rf testBuilt && node_modules/typescript/bin/tsc --module commonjs --outDir testBuilt",
    "test:build": "npm run testBuild && npm run test",
    "watch": "rm -rf appBuilt && echo '\\033[0;32m Stand by for compilation:\\033[m' && node_modules/typescript/bin/tsc --noLib -w --noResolve & npm run serve -- -w"
  },
  "engines": {
    "node": "5.6.0",
    "npm": "3.6.0"
  },
  "bugs": {
    "url": "https://github.com/bamtron5/mean-ngtwo/issues",
    "email": "brandon@brandonam.com"
  },
  "author": {
    "name": "Brandon Morrison",
    "email": "brandon@brandonam.com",
    "url": "http://brandonam.com"
  },
  "dependencies": {
    "acl": "^0.4.9",
    "angular2": "2.0.0-beta.0",
    "babel-preset-es2015": "^6.6.0",
    "bcrypt": "^0.8.5",
    "bindings": "^1.2.1",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.6",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "cookie-session": "^2.0.0-alpha.1",
    "debug": "~2.2.0",
    "ejs": "^2.4.1",
    "ejs-locals": "^1.0.2",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "express": "~4.13.1",
    "express-jwt": "^3.3.0",
    "jasmine-node": "^1.14.5",
    "jsonwebtoken": "^5.5.4",
    "microm": "^0.2.4",
    "mongoose": "^4.3.7",
    "morgan": "~1.6.1",
    "nodemailer": "^2.1.0",
    "passport": "^0.3.2",
    "re-captcha": "0.0.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "serve-favicon": "~2.3.0",
    "systemjs": "^0.19.18",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-watch": "^4.3.5",
    "jasmine-core": "2.4.1"
  }
}
